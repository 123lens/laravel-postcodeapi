{
    "name": "nickurt/laravel-postcodeapi",
    "description": "Universal PostcodeApi for Laravel 5",
    "keywords": ["postcodeapi", "postcode", "api", "universal", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "laravel/framework": "~5.5.0|~5.6.0",
        "guzzlehttp/guzzle": "6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0",
        "guzzlehttp/streams": "3.*"
    },
    "autoload": {
        "psr-4": {
            "nickurt\\PostcodeApi\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "nickurt\\PostcodeApi\\ServiceProvider"
            ],
            "aliases": {
                "PostcodeApi": "nickurt\\PostcodeApi\\Facade"
            }
        }
    }
}
